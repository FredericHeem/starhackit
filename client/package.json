{
  "name": "starhackit-ui",
  "title": "StarHack.It",
  "version": "10.7.1",
  "description": "React + node starter kit. A fullstack boilerplate with authentication and authorisation",
  "homepage": "https://github.com/FredericHeem/starhackit",
  "license": "NLPL",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open --hot",
    "start:prod": "http-server dist/",
    "prestart": "npm run prebuild",
    "lint": "tslint -c tslint.json './src/**/*.{js,jsx,ts,tsx}'",
    "cpd": "jscpd -r console --format 'javascript,typescript' ./src",
    "prebuild": "npm run lint && npm run cpd",
    "build": "webpack --config webpack.prod.js",
    "clean": "rimraf dist coverage",
    "test": "karma start --single-run --no-auto-watch karma.conf.js",
    "test:watch": "karma start --auto-watch --no-single-run karma.conf.js",
    "e2e": "npm run nightwatch",
    "nightwatch": "cd test/nightwatch/ && nightwatch",
    "docker:build": "docker build -t ui .",
    "docker:tag": "docker tag ui fredericheem/ui:latest",
    "docker:push": "docker push fredericheem/ui:latest",
    "docker:btp": "npm run docker:build && npm run docker:tag && npm run docker:push",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FredericHeem/starhackit.git"
  },
  "bugs": {
    "url": "https://github.com/FredericHeem/starhackit/issues"
  },
  "browserslist": "last 2 versions, ie 11",
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@material-ui/core": "4.9.7",
    "@types/react": "16.9.23",
    "axios": "0.19.2",
    "debug": "4.1.1",
    "history": "4.10.1",
    "i18next": "19.3.3",
    "i18next-browser-languagedetector": "4.0.2",
    "i18next-xhr-backend": "3.2.2",
    "intl": "1.2.5",
    "lodash": "4.17.15",
    "mdlean": "0.23.0",
    "mitt": "1.2.0",
    "mobx": "5.15.4",
    "mobx-react": "6.1.8",
    "prop-types": "15.7.2",
    "qs": "6.9.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-leanimate": "0.16.0",
    "react-pagify": "2.4.0",
    "reactabular-table": "8.14.0",
    "segmentize": "0.4.1",
    "universal-router": "9.0.1",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "bluebird": "3.7.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chance": "1.1.4",
    "chromedriver": "80.0.1",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "3.1.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.12.1",
    "imports-loader": "0.8.0",
    "jscpd": "3.1.0",
    "json-loader": "0.5.7",
    "karma": "4.4.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.1",
    "karma-junit-reporter": "2.0.1",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "4.0.2",
    "lodash-webpack-plugin": "0.11.5",
    "mini-css-extract-plugin": "0.9.0",
    "mocha": "7.1.1",
    "nightwatch": "1.3.4",
    "puppeteer": "2.1.1",
    "sinon": "9.0.2",
    "style-loader": "1.1.3",
    "ts-loader": "6.2.1",
    "ts-node": "8.7.0",
    "tslint": "6.1.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "typescript": "3.8.3",
    "url-loader": "4.0.0",
    "webpack": "4.42.0",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-bundle-size-analyzer": "3.1.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-hud": "0.1.2"
  },
  "engines": {
    "node": ">10.x",
    "npm": ">5.x"
  }
}
