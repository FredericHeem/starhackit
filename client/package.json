{
  "name": "starhackit-ui",
  "title": "GruCloud",
  "version": "10.37.0",
  "private": true,
  "description": "Visualize Cloud Infrastructure",
  "license": "NLPL",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js --open",
    "start:admin": "webpack serve --config webpack.dev.js --open-page=admin/users",
    "start:user": "webpack serve --config webpack.dev.js --open-page=user/profile",
    "start:public": "webpack serve --config webpack.dev.js --open-page=public",
    "start:prod": "http-server dist/",
    "cpd": "jscpd -r console --format 'javascript,typescript' ./src",
    "build": "webpack --config webpack.prod.js",
    "clean": "rimraf dist coverage",
    "test": "karma start --single-run --no-auto-watch karma.conf.js",
    "test:watch": "karma start --auto-watch --no-single-run karma.conf.js",
    "nightwatch": "cd test/nightwatch/ && nightwatch",
    "docker:build": "docker build -t ui .",
    "docker:tag": "docker tag ui fredericheem/ui:latest",
    "docker:push": "docker push fredericheem/ui:latest",
    "docker:btp": "npm run docker:build && npm run docker:tag && npm run docker:push",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "browserslist": "last 2 versions, ie 11",
  "dependencies": {
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@material-ui/core": "4.11.4",
    "@types/react": "17.0.5",
    "axios": "0.21.1",
    "date-fns": "2.21.3",
    "debug": "4.3.1",
    "dotenv": "9.0.1",
    "dotenv-cli": "4.0.0",
    "history": "5.0.0",
    "i18next": "20.2.2",
    "i18next-browser-languagedetector": "6.1.0",
    "i18next-xhr-backend": "3.2.2",
    "intl": "1.2.5",
    "lodash": "4.17.21",
    "mdlean": "0.42.0",
    "mitt": "2.1.0",
    "mobx": "6.3.0",
    "mobx-react": "7.1.0",
    "prop-types": "15.7.2",
    "qs": "6.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.2.0",
    "react-pagify": "2.4.0",
    "reactabular-table": "8.14.0",
    "rubico": "1.6.25",
    "segmentize": "0.4.1",
