{
  "name": "starhackit-api",
  "description": "The starhackit backend",
  "version": "5.2.0",
  "private": false,
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:FredericHeem/starhackit.git"
  },
  "license": "NLPL",
  "scripts": {
    "setup": "binci --cleanup && binci env",
    "prestart": "npm run lint",
    "build": "gulp build:production",
    "start": "gulp",
    "pretest": "npm run build",
    "test": "npm run coverage",
    "mocha": "mocha ./src/**/test*.js ./test/**/test*.js",
    "coverage": "istanbul  cover -x 'src/**/test*.js' -x 'test/**/test*.js' -x 'src/plugins/crossbank/*' -x 'src/plugins/fidor/*' _mocha --report text --report html --report lcov ./src/**/test*.js ./test/**/*.js",
    "coveralls": "coveralls < coverage/lcov.info",
    "codecov": "./node_modules/codecov.io/bin/codecov.io.js < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "migrate": "sequelize db:migrate",
    "start:prod": "pm2 start pm2.json --no-daemon",
    "clean": "gulp clean",
    "mocker": "node scripts/mocker-server.js",
    "api-check": "abao src/plugins/users/raml/users.raml --server http://localhost:9000/api/v1",
    "doc": "node scripts/apidoc.js",
    "opendoc": "npm run doc && open build/api/v1/doc/api.html"
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel": "5.8.23",
    "babel-core": "5.8.25",
    "babel-eslint": "7.2.3",
    "babel-root-import": "1.0.1",
    "babel-runtime": "5.8.25",
    "bcryptjs": "2.4.3",
    "bluebird": "3.5.1",
    "chance": "1.0.13",
    "config": "1.30.0",
    "ejs": "2.5.7",
    "eslint": "4.5.0",
    "eslint-plugin-mocha": "4.11.0",
    "gulp": "3.9.1",
    "gulp-babel": "5.2.1",
    "gulp-debug": "2.1.2",
    "gulp-rimraf": "0.2.2",
    "gulp-util": "3.0.8",
    "jsonschema": "1.2.2",
    "jsonwebtoken": "8.2.0",
    "kcors": "2.2.1",
    "koa": "2.5.0",
    "koa-66": "1.0.1",
    "koa-bodyparser": "4.2.0",
    "koa-generic-session": "2.0.1",
    "koa-passport": "4.0.1",
    "koa-redis": "3.1.2",
    "koa-static": "4.0.2",
    "lodash": "4.17.5",
    "logfilename": "0.1.3",
    "moment": "2.21.0",
    "nodemailer": "2.5.0",
    "oauth": "0.9.15",
    "passport-facebook": "2.1.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "passport-oauth2": "1.4.0",
    "pg": "6.0.3",
    "pg-hstore": "2.3.2",
    "pm2": "1.1.3",
    "qs": "6.5.1",
    "rabbitmq-pubsub": "0.6.0",
    "redis": "2.8.0",
    "run-sequence": "1.2.2",
    "sequelize": "4.37.1",
    "sqlite3": "4.0.0",
    "urijs": "1.19.1",
    "winston": "2.4.1"
  },
  "devDependencies": {
    "abao": "0.5.0",
    "async": "2.6.0",
    "binci": "4.2.0",
    "chai": "4.1.2",
    "chalk": "1.1.3",
    "codeclimate-test-reporter": "0.5.0",
    "codecov.io": "0.1.6",
    "coveralls": "2.13.1",
    "isparta": "4.0.0",
    "istanbul": "0.4.5",
    "jsdom": "11.6.2",
    "mocha": "3.5.0",
    "raml-mocker-server": "0.1.6",
    "raml2html": "7.0.0",
    "restauth": "1.1.1",
    "sequelize-cli": "4.0.0",
    "shelljs": "0.8.1",
    "should": "13.2.1",
    "sinon": "4.4.6"
  },
  "engines": {
    "node": ">=8"
  }
}
